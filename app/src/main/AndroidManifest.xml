<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nearur.review">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ricon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ricon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Review" />
        <activity android:name=".Review2" />

        <provider
            android:name=".ReviewData"
            android:authorities="com.nearur.Review.cse"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".Review3" />
        <activity android:name=".AllReviews"></activity>
    </application>

</manifest>